    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.9"
  },
  "dependencies": {
    "gulp-htmlmin": "^4.0.0",
    "normalize-scss": "^7.0.0"
